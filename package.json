    "ant-design-vue": "4.x",
    "axios": "^1.6.7",
    "notivue": "^2.2.1",
    "pinia": "^2.1.7",
    "query-string": "^9.0.0",
    "vue": "^3.4.15",
    "vue-composable": "^1.0.0-beta.24",
